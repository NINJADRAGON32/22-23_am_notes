#count the amount of vowels in the name
name="spongebob squarepants"
#for some variable in a list 
#   do someting

for letter in name:
    if (letter=="a" or letter=="e" or letter =="i" or letter=="o" or letter =="u"):
        numberOfVowels+=1
print(numberOfVowels)

import random
sometimesY=random.randint(2)
for letter in name:
    if sometimesY==1:
        if letter in "aeiou":
            numberOfVowels+=1
    else:
        if letter in "aeiouy":
            numberOfVowels+=1

print(numberOfVowels)